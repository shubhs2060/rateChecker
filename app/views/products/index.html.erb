<p id="notice"><%= notice %></p>
<h3 id="country">Country</h3>
<h1>Products</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Cost($)</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.description %></td>
        <td><%= product.cost %>$</td>
        <td id = "rate<%= product.id %>"><%= link_to 'Check Rate For Your Country', "javascript:void(0);",:onclick => "rateCheck(#{product.cost},#{product.id})" %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Product', new_product_path %>
